program SistemaClinica;

uses
  Vcl.Forms,
  unitPrincipalClinica in 'unitPrincipalClinica.pas' {Form1},
  unitCadastroPacientes in 'unitCadastroPacientes.pas' {Form2},
  unitAgendamentos in 'unitAgendamentos.pas' {Form3},
  unitVisualizarMedicos in 'unitVisualizarMedicos.pas' {Form4};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TForm3, Form3);
  Application.CreateForm(TForm4, Form4);
  Application.Run;
end.
